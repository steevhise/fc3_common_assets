  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js -f table . ",
    "lintfix": "./node_modules/eslint/bin/eslint.js --fix . ",
    "watch:javascript": "webpack --config webpack.config.js --colors --watch",
    "build:javascript": "webpack --config webpack.config.js",
    "build": "babel index.js  -d lib",
    "browsersync": "sleep 5; browser-sync start --proxy localhost:8000 --files ./src/ --reload-delay 1200",
    "serve": "npm run build:javascript && npm run build && node ./lib/index.js",
    "start": "npm run watch:javascript & npm run dev & npm run browsersync",
    "dev:watch": "npm run watch:javascript | npm run dev",
    "dev": "npm run build & nodemon -e js,html --watch index.js --watch ./src/routes --watch ./src/views --watch .src/packages lib/index.js",
    "test": "lab --reporter spec --recursive --timeout 20s"
  },
